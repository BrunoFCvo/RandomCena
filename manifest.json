{
    "name": "Random Cena",
    "description": "John Cena may randomly appear at each page click.",
    "icons": {
		"16": "img/logo_16.png",
		"48": "img/logo_48.png",
		"128": "img/logo_128.png"
	},
    "version": "0.0.2",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "run_at": "document_start",
            "js": ["content.js"]
        }
	],
	"web_accessible_resources": ["cena.mp4"]
}
